            <endpoint address="http://localhost/WhoIsBetter_WSSOAP/UsuarioService.svc"
                binding="customBinding" bindingConfiguration="CustomBinding_IUsuarioService"
                contract="WSUsuario.IUsuarioService" name="CustomBinding_IUsuarioService" />
